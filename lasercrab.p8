end

function intro()
	cls()
	ltick=-900
	spr(64,20,10,11,2)
	if time() == 2 then sfx(5) end
	if time() > 2 and time() < 4 then print("controller highly recommended",5,60) spr(12,56,40,2,2) end
	if time() > 4 then print("friend required",35,60) spr(44,60,40,1,2) end
	if time() < 2 then print("A GAME BY emerald585",25,60) spr(45,60,40,1,2) end
end

function effect(x,y,vx,vy,c,s)
	local fx = {
		x=x,
		y=y,
		vx=vx,
		vy=vy,
		c=c,
		s=s,
	}
	add(particle,fx)
end

function fxupdate()
	for fx in all(particle) do
		fx.x += fx.vx
		fx.y += fx.vy
		fx.s -= .1
		if fx.s < 0 then del(particle, fx) end
	end
end


function newitem(x,y,t)
	local i = {
	x=x,
	y=y,
	vy=0,
	t=t}
	add(item,i)
end

function updateitem()
	for i in all(item) do
		if tcol(i.x,i.y+4,0) then i.vy-=i.vy*1.5 else i.vy += 0.1end
		i.y+=i.vy
		if i.t == 56 then
			if prc(i,p) then sfx(2)p.hp += 10 del(item,i)
			elseif prc(i,p2) then sfx(2)p2.hp += 10 del(item,i) end
		elseif i.t == 57 then
			if prc(i,p) then sfx(2)p.pwr=1 p.pwrt=70 del(item,i)
			elseif prc(i,p2) then sfx(2) p2.pwr=1 p2.pwrt=70 del(item,i) end
		end
	end
end

function easeoutinquart(t)
	if t<.5 then
		t-=.5
		return .5-8*t*t*t*t
	else
		t-=.5
		return .5+8*t*t*t*t
	end
end


function prc(p,r)
  return p.x >= r.x and
         p.x <= r.x+r.w and
         p.y >= r.y and
         p.y <= r.y+r.h
end

function tcol(x,y,flag)
	return fget(mget(x\8,y\8),flag)
end

function shoot(x,y,vx,vy,c)
	local b = {  
		x=x,
		y=y,
		vx=vx,
		vy=vy,
		c=c
	}
	add(bullet,b)
	sfx(0)
end

function bullet_update()
	for b in all(bullet) do
		b.x += b.vx
		b.y += b.vy
		if b.x > 130 then del(bullet, b) end
		if b.x < 0 then del(bullet, b) end
		if prc(b,p2) then sfx(1)del(bullet,b) if p2.hp < 0 then p2.vx+=b.vx else p2.hp-=1end
		elseif prc(b,p) then sfx(1)del(bullet,b) if p.hp < 0 then p.vx+=b.vx else p.hp -= 1 end end
		
	end
end

